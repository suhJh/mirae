<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="test">

	<insert id="test_insert" parameterType="java.util.Map">
		insert into
			test(seq, data)
			values(#seq#, #data#)
	</insert>


	<select id="test_select" resultType="java.util.HashMap">
		select * from test
	</select>


</mapper>